@model List<Speciality>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @{
        foreach (var speciality in Model)
        {
            <div>
                <a href="/specialities/getservices/@speciality.Id">@speciality.NameSpeciality</a>
            </div>
        }
    }
</div>
